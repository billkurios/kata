  "servers": [
    {
      "description": "Test Server (Local)",
      "url": "http://127.0.0.1:3000"
    }
  ],
  "tags": [
    {
      "name": "Properties",
      "description": "Endpoints related to properties portfolio"
    }
  ],
  "paths": {
    "/api/manager/properties": {
      "get": {
        "description": "Get a list of properties linked to the given manager.",
        "responses": {
          "400": {
            "description": "Manager Identifier"
          },
          "404": {
            "description": "manager_id don't exist"
          },
          "200": {
            "description": "Should return a list",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Property"
                }
              }
            }
          }
        }
      }
    }
  },
  "openapi": "3.0.0",
  "components": {
    "schemas": {
      "Property": {
        "type": "object",
        "properties": {
          "number_bedrooms": {
            "type": "integer"
          },
          "price": {
            "type": "integer"
          },
          "id": {
            "type": "integer"
          },
          "location": {
            "type": "string",
            "maxLength": 100
          },
          "type": {
            "type": "string",
            "maxLength": 100
          }
        },
        "required": [
          "location",
          "number_bedrooms",
          "price",
          "type"
        ]
      }
    }
  }
}